<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-6">
        <div class="padding-nav-card">
          <div class="card-accepted">
            <h2>Solicitud Confirmada</h2>
            <div class="mb-4 text-center">
              <img class="avatar-large-c" alt="avatar-large" src="https://kitt.lewagon.com/placeholder/users/arthur-littm" /> <%# aqui va la imagen del perfil %>
            </div>
            <div class="mb-4 text-center">
              <i class="fas fa-check-circle check-circle-green"></i>
            </div>
            <% @users.each do |user| %>
              <p><%= user.first_name %></p>
              <p><%= user.last_name %></p>
            <% end %>
            <% @services.each do |s| %>
              <h3><%= s.category %></h3> <%# aqui va el nombre de la categoría %>
            <% end %>

            <p><% 3.times do |i| %>
              <span class="fas fa-star star-yellow"></span>
            <% end %></p> <%# aqui va la calificación %>
            
            <p>Tiempo de espera estimada</p>
            <p>20 Minutos</p>
            
          </div>
        </div>
    </div>
  </div>
</div>